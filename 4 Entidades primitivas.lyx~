\begin_inset Separator parbreak
\end_inset


\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Operadores aritméticos
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize
Se utilizan con tipos de datos numéricos
\end_layout

\begin_layout Itemize
+ , - , * , / , % , ^
\end_layout

\begin_layout Itemize
Orden de prioridad
\end_layout

\begin_deeper
\begin_layout Itemize
(paréntesis, corchetes, llaves), luego las de exponenciaciones, luego las
 multiplicaciones y divisiones y, por último, las sumas y las restas.
 
\end_layout

\end_deeper
\end_deeper
\begin_layout Standard
\begin_inset Separator parbreak
\end_inset


\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Operadores relacionales
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize
Establecen o verifican clasificaciones entre números u otro tipo de valores
 (caracteres, cadenas, ...)
\end_layout

\begin_layout Itemize
Todo tipo de dato susceptible de ser ordenado por cualquier criterio puede
 ser comparado con estos operadores; como los anteriores devuelven un valor
 de verdad en función del resultado que tenga la comparación en cada caso.
 
\end_layout

\begin_layout Itemize
El resultado es un valor tipo booleano
\end_layout

\begin_layout Itemize
\begin_inset Formula $<,>,<=,>=,<>,=$
\end_inset


\end_layout

\end_deeper
