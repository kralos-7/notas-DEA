\begin_inset Argument 4
status open

\begin_layout Plain Layout
Identificadores
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize
Es una secuencia de caracteres alfabéticos, numéricos y el guión bajo
\end_layout

\begin_layout Itemize
Con ellos podemos dar nombre a variables, constantes, tipos de datos, nombre
 de funciones o procedimientos, etcétera
\end_layout

\begin_layout Itemize
El programador tiene la libertad de darle cualquier nombre a un identificador
\end_layout

\end_deeper
\begin_layout Standard
\begin_inset Separator parbreak
\end_inset


\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

